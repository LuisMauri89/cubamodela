<% if @success %>
	$('#notification-messages-container').html("<%= j render 'shared/action_info', message: 'views.admin.coupons.messages.coupons_generation_success' %>");
	$('#coupons-list-container').empty();
	$('#coupons-list-container').html("<%= j render 'coupons' %>");
<% else %>
	$('#notification-messages-container').html("<%= j render 'shared/error_info', message: 'views.admin.coupons.messages.coupons_generation_failed' %>");
<% end %>