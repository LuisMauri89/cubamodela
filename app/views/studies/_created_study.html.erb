<div class="row  panel panel-default panel-study" id="panel-<%= @study.id %>">
  <div class="col-md-12" id="panel-<%= @study.id %>-container">
    <div class="row">
      <div class="col-md-12" id="form-edit-study-<%= @study.id %>">
      </div>
    </div>
    <div class="row row-study-<%= @study.id %>">
      <div class="col-md-5">
        <h4 id="study-title-<%= @study.id %>">
          <%= @study.title %>
        </h4>
      </div>
      <div class="col-md-5 text-center">
        <h4 id="study-place-<%= @study.id %>">
          Place: <%= @study.place %>
        </h4>
      </div>
      <div class="col-md-2" style="padding-top: 5px;">
        <%= link_to 'Delete', study_path(@study), method: :delete, remote: true, class: "btn btn-danger btn-xs pull-right" %>
        <%= link_to 'Edit', edit_study_path(@study), remote: true, class: "btn btn-success btn-xs pull-right" %>
      </div>
    </div>
    <div class="row row-study-<%= @study.id %>">
      <div class="col-md-12">
        <p id="study-desc-<%= @study.id %>">
          <%= @study.description %>
        </p>
      </div>
    </div>
  </div>
</div>