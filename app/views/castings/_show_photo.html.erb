<div id="photo-modal-large" class="modal-dialog">
  <div class="modal-content">
  <div class="modal-header">
    <h3>Photo</h3>
  </div>
  <div class="modal-body">
    <div id="model-modal-info-container"></div>
    <%= image_tag @photo.image.url, alt: @photo.image.url, class: "img-responsive img-center-horizontal" %>
  </div>
  <div class="modal-footer">
    <%= link_to "Close", "#", class: "btn btn-primary", id: "btn-dismiss-show-photo", data: { dismiss: "modal" } %>
  </div>
  </div>
</div>