<% if @intent.errors.any? %>
	$('#notification-messages-container').html("<%= j render 'errors_intent' %>");
<% else %>
	$('#btn-apply-<%= @casting.id %><%= @profile.id %>').remove();
	$('#notification-messages-container').html("<%= j render 'action_info', message: 'views.castings.messages.associations.applied' %>");
<% end %>