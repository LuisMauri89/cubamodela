<% if @intent.errors.any? %>
	$('#notification-messages-container').html("<%= j render 'errors_manage' %>");
<% else %>
	$('#notification-messages-container').html("<%= j render 'action_info', text: 'Model invited' %>");
	<% if @list_item == "applied" %>
		$('#model-<%= @casting.id %>-<%= @profile.id %>').remove();
	<% end %>
<% end %>