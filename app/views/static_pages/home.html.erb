    <div class="row row-no-space">
        <div class="col-md-12 cell-no-space">
            <div id="subscribe-container" class="notify relative-position">
              <%= image_tag "cubamodela_home_subscribe_cuba_models_en.jpg", alt: "Photo", class: "img-responsive" %>
              <div id="home-subscribe-form-container">
                <%= render "home_sign_up" %>
              </div>
            </div>
        </div>
    </div>
<% end %>

