<!DOCTYPE html>
<html>
  <head>
    <title>Modelsincuba</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>

    <div id="main-container" class="container-fluid">
      <div class="row">
        <div id="flash-messages-container" class="col-md-12">
          <%= render 'layouts/flash_messages' %>
        </div>
      </div>
      <div class="row">
        <div id="banner-container" class="col-md-12">
          <%= render 'layouts/banner_home_page' %>
        </div>
      </div>

      <div class="row">
        <div id="navbar-container" class="col-md-12">
          <%= render 'layouts/navbar_home_page' %>
        </div>
      </div>

      <%= yield %>
    </div>
    
  </body>
</html>
