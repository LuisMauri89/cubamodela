<div id="home-banner-carousel" class="carousel slide" data-interval="10000" data-ride="carousel">
	<!-- Indicators -->
	<ol class="carousel-indicators">
		<li data-target="home-banner-carousel" data-slide-to="0" class="active"></li>
		<li data-target="home-banner-carousel" data-slide-to="1"></li>
		<li data-target="home-banner-carousel" data-slide-to="2"></li>
	</ol>

	<!-- Wrapper for slide -->
	<div class="carousel-inner" role="listbox">

		<!-- Slide 0 -->
		<div class="item active">
			<%= image_tag "5.JPG", alt: "Banner Home Page" %>
			<div class="carousel-caption">
				<div class="row">
        			<div class="col-md-12">
						<h1 class="text-center" data-animation="animated fadeInLeft" data-delay="0.5s">Find models that fits your needs</h1>
					</div>
				</div>
				<div class="row">
        			<div class="col-md-3">
        				<div class="banner-thumbnail-image">
							<%= image_tag "2.JPG", class: "img-responsive img-thumbnail", alt: "Models", data:{ animation: "animated bounceInRight", delay: "1s" } %>
						</div>
					</div>
					<div class="col-md-3">
						<div class="banner-thumbnail-image">
							<%= image_tag "1.JPG", class: "img-responsive img-thumbnail", alt: "Models", data:{ animation: "animated bounceInRight", delay: "1.5s" } %>
						</div>
					</div>
        			<div class="col-md-3">
        				<div class="banner-thumbnail-image">
							<%= image_tag "3.JPG", class: "img-responsive img-thumbnail", alt: "Models", data:{ animation: "animated bounceInRight", delay: "2s" } %>
						</div>
					</div>
					<div class="col-md-3">
						<div class="banner-thumbnail-image">
							<%= image_tag "4.JPG", class: "img-responsive img-thumbnail", alt: "Models", data:{ animation: "animated bounceInRight", delay: "2.5s" } %>
						</div>
					</div>
				</div>
				<div class="row">
        			<div class="col-md-12">
						<%= link_to "Find out more...", profile_models_path, class: "btn btn-success btn-lg", data:{ animation: "animated rotateInDownLeft", delay: "3s" } %>
					</div>
				</div>
			</div>
		</div>

		<!-- Slide 1 -->
		<div class="item">
			<%= image_tag "10.JPG", alt: "Banner Home Page" %>
			<div class="carousel-caption">
				<div class="row">
        			<div class="col-md-12">
						<h1 class="text-center" data-animation="animated fadeInLeft" data-delay="0.5s">Find photographers width amazing talent</h1>
