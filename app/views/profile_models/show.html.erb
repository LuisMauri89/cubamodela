<% if @profile.reviewed %>
	<% if !current_user.nil? && current_user.admin? %>
		<div class="row">
			<div class="col-md-12">
				<div id="actions-container">
				    <%= render 'admin_actions_container' %>
				</div>
			</div>
		</div>
	<% end %>

	<%= render 'show' %>
<% else %>
	<% if !current_user.nil? && current_user.admin? %>
		<div class="row">
			<div class="col-md-12">
				<div id="actions-container">
				    <%= render 'admin_actions_container' %>
				</div>
			</div>
		</div>

		<%= render 'show' %>
	<% else %>
		<h1><%= t('views.profile_models.show.pending_review_text') %></h1>
	<% end %>
<% end %>